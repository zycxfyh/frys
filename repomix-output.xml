          return value;
        }
        cache.delete(key);
      }

      // æ‰§è¡ŒåŸæ–¹æ³•
      const result = await originalMethod.apply(this, args);

      // ç¼“å­˜ç»“æœ
      cache.set(key, { value: result, timestamp: now });

      return result;
    };

    return descriptor;
  };
}
