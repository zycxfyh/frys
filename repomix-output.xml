    console.log(`   âœ… é€šè¿‡: ${this.results.passed}`);
    console.log(`   âŒ å¤±è´¥: ${this.results.failed}`);
    console.log(`   âš ï¸  è­¦å‘Š: ${this.results.warnings}`);

