        "type": "stat",
        "targets": [
          {
            "expr": "slo:api_latency:p95",
            "legendFormat": "P95 å“åº”æ—¶é—´"
          }
        ]
      },
      {
        "title": "é”™è¯¯ç‡",
        "type": "stat",
        "targets": [
          {
            "expr": "slo:error_rate:ratio",
            "legendFormat": "é”™è¯¯ç‡"
          }
        ]
      },
      {
        "title": "é”™è¯¯é¢„ç®—å‰©ä½™",
        "type": "stat",
        "targets": [
          {
            "expr": "slo:error_budget:remaining",
            "legendFormat": "é”™è¯¯é¢„ç®—å‰©ä½™"
          }
        ]
      }
