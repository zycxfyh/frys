/**
 * WokeFlow Production - æ—¥å¿—å·¥å…·
 */

import { createWriteStream } from 'fs';
import { dirname } from 'path';
import { fileURLToPath } from 'url';
import { mkdirSync } from 'fs';

import { config } from './config.js';

const __filename = fileURLToPath(import.meta.url);
const __dirname = dirname(__filename);

// æ—¥å¿—çº§åˆ«å®šä¹‰
const LOG_LEVELS = {
  error: 0,
  warn: 1,
  info: 2,
  debug: 3,
};

// ANSI é¢œè‰²ä»£ç 
const colors = {
  reset: '\x1b[0m',
  red: '\x1b[31m',
  yellow: '\x1b[33m',
  green: '\x1b[32m',
  blue: '\x1b[34m',
  magenta: '\x1b[35m',
  cyan: '\x1b[36m',
};

// æ—¥å¿—æ ¼å¼åŒ–å™¨
class LogFormatter {
  static format(level, message, meta = {}) {
    const timestamp = new Date().toISOString();
    const levelUpper = level.toUpperCase();

    if (config.logging.format === 'json') {
      return JSON.stringify({
        timestamp,
        level,
        message,
        ...meta,
      });
    } else {
      // æ§åˆ¶å°æ ¼å¼
      const color = this.getColor(level);
      const prefix = `${color}[${timestamp}] ${levelUpper}${colors.reset}`;
      const metaStr =
        Object.keys(meta).length > 0 ? ` ${JSON.stringify(meta)}` : '';
      return `${prefix} ${message}${metaStr}`;
    }
  }

  static getColor(level) {
    switch (level) {
      case 'error':
        return colors.red;
      case 'warn':
        return colors.yellow;
      case 'info':
        return colors.green;
      case 'debug':
        return colors.blue;
      default:
        return colors.reset;
    }
  }
}

// æ–‡ä»¶æ—¥å¿—å†™å…¥å™¨
class FileLogger {
  constructor(filePath) {
    this.filePath = filePath;
    this.maxSize = this.parseSize(config.logging.transports.file.maxSize);
    this.maxFiles = parseInt(config.logging.transports.file.maxFiles);
    this.currentSize = 0;
    this.fileIndex = 0;

    // ç¡®ä¿æ—¥å¿—ç›®å½•å­˜åœ¨
    const logDir = dirname(filePath);
    try {
      mkdirSync(logDir, { recursive: true });
    } catch (error) {
      if (error.code !== 'EEXIST') {
        console.error('åˆ›å»ºæ—¥å¿—ç›®å½•å¤±è´¥:', error);
      }
    }

    this.stream = createWriteStream(this.getCurrentFilePath(), { flags: 'a' });
  }

  write(message) {
    const line = message + '\n';
    const lineSize = Buffer.byteLength(line, 'utf8');

    // æ£€æŸ¥æ˜¯å¦éœ€è¦è½®è½¬æ–‡ä»¶
    if (this.currentSize + lineSize > this.maxSize) {
      this.rotate();
    }

    this.stream.write(line);
    this.currentSize += lineSize;
  }

  rotate() {
    this.stream.end();

    // é‡å‘½åç°æœ‰æ–‡ä»¶
    for (let i = this.maxFiles - 1; i > 0; i--) {
      const _oldPath = this.getFilePath(i - 1);
      const _newPath = this.getFilePath(i);
      try {
        // è¿™é‡Œå¯ä»¥æ·»åŠ æ–‡ä»¶é‡å‘½åé€»è¾‘
      } catch (error) {
        // å¿½ç•¥é‡å‘½åé”™è¯¯
      }
    }

    // åˆ›å»ºæ–°çš„æ—¥å¿—æ–‡ä»¶
    this.currentSize = 0;
    this.stream = createWriteStream(this.getCurrentFilePath(), { flags: 'w' });
  }

  getCurrentFilePath() {
    return this.getFilePath(0);
  }

  getFilePath(index) {
    if (index === 0) {
      return this.filePath;
    }
    const ext = this.filePath.split('.').pop();
    const base = this.filePath.slice(0, -ext.length - 1);
    return `${base}.${index}.${ext}`;
  }

  parseSize(sizeStr) {
    const units = {
      k: 1024,
      m: 1024 * 1024,
      g: 1024 * 1024 * 1024,
    };

    const match = sizeStr.toLowerCase().match(/^(\d+)([kmg]?)$/);
    if (!match) return 10 * 1024 * 1024; // é»˜è®¤10MB

    const [, num, unit] = match;
    return parseInt(num) * (units[unit] || 1);
  }

  close() {
    if (this.stream) {
      this.stream.end();
    }
  }
}

// ä¸»æ—¥å¿—å™¨ç±»
class Logger {
  constructor() {
    this.level = LOG_LEVELS[config.logging.level] || LOG_LEVELS.info;
    this.transports = [];

    // åˆå§‹åŒ–ä¼ è¾“å™¨
    this.initTransports();
  }

  initTransports() {
    // æ§åˆ¶å°ä¼ è¾“å™¨
    if (config.logging.transports.console.enabled) {
      this.transports.push({
        name: 'console',
        write: (level, message, meta) => {
          const formatted = LogFormatter.format(level, message, meta);
          if (level === 'error') {
            console.error(formatted);
          } else {
            console.log(formatted);
          }
        },
      });
    }

    // æ–‡ä»¶ä¼ è¾“å™¨
    if (config.logging.transports.file.enabled) {
      const fileLogger = new FileLogger(config.logging.transports.file.path);
      this.transports.push({
        name: 'file',
        write: (level, message, meta) => {
          const formatted = LogFormatter.format(level, message, meta);
          fileLogger.write(formatted);
        },
      });
    }
  }

  log(level, message, meta = {}) {
    if (LOG_LEVELS[level] > this.level) {
      return; // ä½äºå½“å‰æ—¥å¿—çº§åˆ«ï¼Œè·³è¿‡
    }

    // å†™å…¥æ‰€æœ‰ä¼ è¾“å™¨
    this.transports.forEach((transport) => {
      try {
        transport.write(level, message, meta);
      } catch (error) {
        // é¿å…æ—¥å¿—ç³»ç»Ÿæœ¬èº«çš„é”™è¯¯å½±å“åº”ç”¨
        console.error('æ—¥å¿—å†™å…¥å¤±è´¥:', error);
      }
    });
  }

  error(message, error = null, meta = {}) {
    const errorMeta = { ...meta };
    if (error) {
      if (error instanceof Error) {
        errorMeta.error = {
          name: error.name,
          message: error.message,
          stack: error.stack,
        };
      } else {
        errorMeta.error = error;
      }
    }
    this.log('error', message, errorMeta);
  }

  warn(message, meta = {}) {
    this.log('warn', message, meta);
  }

  info(message, meta = {}) {
