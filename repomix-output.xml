          runbook_url: "https://docs.wokeflow.com/runbooks/high-cpu"

  # åŸºç¡€è®¾æ–½å‘Šè­¦
  - name: infrastructure_alerts
    rules:
      # æ•°æ®åº“è¿æ¥å¤±è´¥
      - alert: DatabaseDown
        expr: pg_up == 0
        for: 1m
        labels:
          severity: critical
          service: database
        annotations:
          summary: "æ•°æ®åº“è¿æ¥å¤±è´¥"
