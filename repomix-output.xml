<file path="monitoring/prometheus/prometheus.yml">
global:
  scrape_interval: 15s
  evaluation_interval: 15s

rule_files:
  # - "first_rules.yml"
  # - "second_rules.yml"

scrape_configs:
  - job_name: 'wokeflow'
    static_configs:
      - targets: ['wokeflow:3000']
    scrape_interval: 5s
    metrics_path: '/metrics'

  - job_name: 'redis'
