      expect(response.success).toBe(true);
      expect(retryCount).toBe(0); // åœ¨è¿™ä¸ªæµ‹è¯•ä¸­æˆ‘ä»¬ä¸éœ€è¦é‡è¯•
    });
  });

  describe('è·¨æ¨¡å—çŠ¶æ€åŒæ­¥', () => {
    it('åº”è¯¥åœ¨HTTPè¯·æ±‚ä¸­åŒæ­¥è®¤è¯çŠ¶æ€', async () => {
      const instance = http.create({
