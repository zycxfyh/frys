  topologicalSort() {
    // ç®€å•æŒ‰æ–‡ä»¶åæ’åºï¼Œç¡®ä¿åŸºç¡€æ¨¡å—åœ¨å‰
    const priorityModules = ['WokeFlowError', 'ErrorHandler'];
    const otherModules = this.coreModules.filter(m => !priorityModules.includes(m.name));

