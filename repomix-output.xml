 * å€Ÿé‰´ Prettier çš„æ ¸å¿ƒç†å¿µ
 */
class PrettierInspiredFormatting {
  /**
   * æ„é€ å‡½æ•°
   * åˆå§‹åŒ–PrettierInspiredFormattingç®¡ç†å™¨
   */
  constructor() {
    this.configs = new Map(); // é…ç½®
    this.parsers = new Map(); // è§£æå™¨
    this.formatted = []; // æ ¼å¼åŒ–å†å²
  }

  /**
   * æ ¼å¼åŒ–ä»£ç 
   * @param {string} code - åŸå§‹ä»£ç 
   * @param {string} language - ç¼–ç¨‹è¯­è¨€
   * @param {Object} options - æ ¼å¼åŒ–é€‰é¡¹
   * @returns {string} æ ¼å¼åŒ–åçš„ä»£ç 
   */
  format(code, language, options = {}) {
    const formattedCode = this.applyFormatting(code, language, options);

    this.formatted.push({
      original: code,
      formatted: formattedCode,
      language,
      options,
      timestamp: new Date(),
    });

    console.log(`âœ¨ ä»£ç å·²æ ¼å¼åŒ–: ${language}`);
    return formattedCode;
  }

  /**
   * åº”ç”¨æ ¼å¼åŒ–è§„åˆ™
   * @param {string} code - ä»£ç 
   * @param {string} language - è¯­è¨€
   * @param {Object} options - é€‰é¡¹
   * @returns {string} æ ¼å¼åŒ–åçš„ä»£ç 
   */
  applyFormatting(code, language, options) {
    let formatted = code;

    // åŸºæœ¬æ ¼å¼åŒ–è§„åˆ™
    switch (language) {
      case 'javascript':
      case 'typescript':
        formatted = this.formatJavaScript(formatted, options);
        break;
      case 'json':
        formatted = this.formatJSON(formatted, options);
        break;
      case 'css':
        formatted = this.formatCSS(formatted, options);
        break;
      default:
        console.warn(`ä¸æ”¯æŒçš„è¯­è¨€: ${language}`);
    }

    return formatted;
  }

  /**
   * æ ¼å¼åŒ–JavaScriptä»£ç 
   * @param {string} code - JSä»£ç 
   * @param {Object} options - é€‰é¡¹
   * @returns {string} æ ¼å¼åŒ–åçš„ä»£ç 
   */
  formatJavaScript(code, _options) {
    // ç®€åŒ–å®ç°ï¼Œå®é™…åº”è¯¥ä½¿ç”¨æ›´å¤æ‚çš„æ ¼å¼åŒ–é€»è¾‘
    return code
      .replace(/\s*;\s*/g, ';\n') // åˆ†å·åæ¢è¡Œ
      .replace(/\s*{\s*/g, ' {\n') // å·¦å¤§æ‹¬å·å‰æ¢è¡Œ
      .replace(/\s*}\s*/g, '\n}\n') // å³å¤§æ‹¬å·åæ¢è¡Œ
      .replace(/\n\s*\n/g, '\n'); // ç§»é™¤å¤šä½™ç©ºè¡Œ
  }

  /**
   * æ ¼å¼åŒ–JSON
   * @param {string} code - JSONå­—ç¬¦ä¸²
   * @param {Object} options - é€‰é¡¹
   * @returns {string} æ ¼å¼åŒ–åçš„JSON
   */
  formatJSON(code, options) {
    try {
      const obj = JSON.parse(code);
      return JSON.stringify(obj, null, options.indent || 2);
    } catch (error) {
      console.error('JSONæ ¼å¼åŒ–å¤±è´¥:', error);
      return code;
    }
  }

  /**
   * æ ¼å¼åŒ–CSS
   * @param {string} code - CSSä»£ç 
   * @param {Object} options - é€‰é¡¹
   * @returns {string} æ ¼å¼åŒ–åçš„CSS
   */
  formatCSS(code, _options) {
    // ç®€åŒ–å®ç°
    return code
      .replace(/\s*{\s*/g, ' {\n  ')
      .replace(/\s*;\s*/g, ';\n  ')
      .replace(/\s*}\s*/g, '\n}\n');
  }

  /**
   * è·å–ç»Ÿè®¡ä¿¡æ¯
   * @returns {Object} ç»Ÿè®¡æ•°æ®
   */
  getStats() {
    return {
      configs: this.configs.size,
      parsers: this.parsers.size,
      formattedCount: this.formatted.length,
    };
  }
}

export default PrettierInspiredFormatting;
