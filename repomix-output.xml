/**
 * WokeFlow Production - ç”¨æˆ·æœåŠ¡
 */

import { logger } from '../utils/logger.js';
import { config } from '../utils/config.js';

export class UserService {
  constructor(dependencies) {
    this.http = dependencies.http;
    this.auth = dependencies.auth;
    this.state = dependencies.state;
    this.messaging = dependencies.messaging;

    this.users = new Map();
    this.sessions = new Map();
  }

  async start() {
    logger.info('å¯åŠ¨ç”¨æˆ·æœåŠ¡');

    // è®¢é˜…ç”¨æˆ·ç›¸å…³äº‹ä»¶
    await this.setupEventListeners();

    logger.info('ç”¨æˆ·æœåŠ¡å¯åŠ¨å®Œæˆ');
  }

  async stop() {
    logger.info('åœæ­¢ç”¨æˆ·æœåŠ¡');

    // æ¸…ç†æ‰€æœ‰ç”¨æˆ·ä¼šè¯
    for (const [sessionId, _session] of this.sessions) {
      await this.endSession(sessionId);
    }

    logger.info('ç”¨æˆ·æœåŠ¡å·²åœæ­¢');
  }

  async setupEventListeners() {
    // ç›‘å¬ç”¨æˆ·è®¤è¯äº‹ä»¶
    this.messaging.subscribe('user.login', async (event) => {
      await this.handleLogin(event);
    });

    this.messaging.subscribe('user.logout', async (event) => {
      await this.handleLogout(event);
    });

    // ç›‘å¬ç”¨æˆ·ç®¡ç†äº‹ä»¶
    this.messaging.subscribe('user.create', async (event) => {
      await this.createUser(event.userData);
    });

    this.messaging.subscribe('user.update', async (event) => {
      await this.updateUser(event.userId, event.userData);
    });

    this.messaging.subscribe('user.delete', async (event) => {
      await this.deleteUser(event.userId);
    });
  }

  async createUser(userData) {
    try {
      // éªŒè¯ç”¨æˆ·æ•°æ®
      this.validateUserData(userData);

      // æ£€æŸ¥ç”¨æˆ·åæ˜¯å¦å·²å­˜åœ¨
      const existingUser = this.findUserByUsername(userData.username);
      if (existingUser) {
        throw new Error(`ç”¨æˆ·åå·²å­˜åœ¨: ${userData.username}`);
      }

      // åˆ›å»ºç”¨æˆ·å¯¹è±¡
      const userId = this.generateUserId();
      const user = {
        id: userId,
        username: userData.username,
        email: userData.email,
        fullName: userData.fullName,
        role: userData.role || 'user',
        status: 'active',
        createdAt: new Date(),
        updatedAt: new Date(),
        lastLoginAt: null,
        loginCount: 0,
      };

      // å“ˆå¸Œå¯†ç 
      user.passwordHash = await this.hashPassword(userData.password);

      // ä¿å­˜åˆ°çŠ¶æ€ç®¡ç†
      this.state.setState((state) => ({
        users: new Map(state.users).set(userId, user),
      }));

      this.users.set(userId, user);

      logger.info('ç”¨æˆ·åˆ›å»ºæˆåŠŸ', {
        userId,
        username: user.username,
        email: user.email,
      });

      // å‘å¸ƒç”¨æˆ·åˆ›å»ºäº‹ä»¶
      await this.messaging.publish('user.created', {
        userId,
        user: { ...user, passwordHash: undefined }, // ä¸åŒ…å«å¯†ç å“ˆå¸Œ
      });

      return { userId, user: { ...user, passwordHash: undefined } };
    } catch (error) {
      logger.error('ç”¨æˆ·åˆ›å»ºå¤±è´¥', error, { userData });
      throw error;
    }
  }

  async authenticateUser(username, password) {
    try {
      const user = this.findUserByUsername(username);
      if (!user) {
        throw new Error('ç”¨æˆ·ä¸å­˜åœ¨');
      }

      if (user.status !== 'active') {
        throw new Error('ç”¨æˆ·è´¦æˆ·å·²è¢«ç¦ç”¨');
      }

      // éªŒè¯å¯†ç 
      const isValidPassword = await this.verifyPassword(
        password,
        user.passwordHash,
      );
      if (!isValidPassword) {
        throw new Error('å¯†ç é”™è¯¯');
      }

      // ç”ŸæˆJWTä»¤ç‰Œ
      const token = this.auth.generateToken({
        userId: user.id,
        username: user.username,
        email: user.email,
        role: user.role,
      });

      // åˆ›å»ºä¼šè¯
      const sessionId = this.generateSessionId();
      const session = {
        id: sessionId,
        userId: user.id,
        token,
        createdAt: new Date(),
        lastActivity: new Date(),
        ipAddress: null, // å¯ä»¥ä»è¯·æ±‚ä¸­è·å–
        userAgent: null,
      };

      this.sessions.set(sessionId, session);

      // æ›´æ–°ç”¨æˆ·æœ€åç™»å½•ä¿¡æ¯
      user.lastLoginAt = new Date();
      user.loginCount += 1;
      user.updatedAt = new Date();

      this.updateUserState(user.id, user);

      logger.info('ç”¨æˆ·è®¤è¯æˆåŠŸ', {
        userId: user.id,
        username: user.username,
        sessionId,
      });

      // å‘å¸ƒç™»å½•äº‹ä»¶
      await this.messaging.publish('user.authenticated', {
        userId: user.id,
        sessionId,
        token,
      });

      return {
        user: { ...user, passwordHash: undefined },
        token,
        sessionId,
      };
    } catch (error) {
      logger.error('ç”¨æˆ·è®¤è¯å¤±è´¥', error, { username });
      throw error;
    }
  }

  async validateToken(token) {
    try {
      const decoded = this.auth.verifyToken(token);

      // æ£€æŸ¥ç”¨æˆ·æ˜¯å¦ä»ç„¶æœ‰æ•ˆ
      const user = this.users.get(decoded.userId);
      if (!user || user.status !== 'active') {
        throw new Error('ç”¨æˆ·ä¸å­˜åœ¨æˆ–å·²è¢«ç¦ç”¨');
      }

      return { valid: true, user, decoded };
    } catch (error) {
      logger.warn('ä»¤ç‰ŒéªŒè¯å¤±è´¥', { error: error.message });
      return { valid: false, error: error.message };
    }
  }

  async logout(sessionId) {
    try {
      const session = this.sessions.get(sessionId);
      if (!session) {
        return; // ä¼šè¯ä¸å­˜åœ¨ï¼Œé™é»˜å¤„ç†
      }

      // ç»“æŸä¼šè¯
      await this.endSession(sessionId);

      logger.info('ç”¨æˆ·ç™»å‡ºæˆåŠŸ', {
        userId: session.userId,
        sessionId,
      });

      // å‘å¸ƒç™»å‡ºäº‹ä»¶
      await this.messaging.publish('user.logged_out', {
        userId: session.userId,
        sessionId,
      });
    } catch (error) {
      logger.error('ç”¨æˆ·ç™»å‡ºå¤±è´¥', error, { sessionId });
      throw error;
    }
  }

  async updateUser(userId, userData) {
    try {
      const user = this.users.get(userId);
      if (!user) {
        throw new Error(`ç”¨æˆ·ä¸å­˜åœ¨: ${userId}`);
      }

      // æ›´æ–°ç”¨æˆ·æ•°æ®
      const updatedUser = {
        ...user,
        ...userData,
        updatedAt: new Date(),
      };

      // å¦‚æœæ›´æ–°å¯†ç ï¼Œéœ€è¦é‡æ–°å“ˆå¸Œ
      if (userData.password) {
        updatedUser.passwordHash = await this.hashPassword(userData.password);
      }

      this.users.set(userId, updatedUser);
      this.updateUserState(userId, updatedUser);

      logger.info('ç”¨æˆ·ä¿¡æ¯æ›´æ–°æˆåŠŸ', {
        userId,
        username: updatedUser.username,
      });

      // å‘å¸ƒæ›´æ–°äº‹ä»¶
      await this.messaging.publish('user.updated', {
        userId,
        user: { ...updatedUser, passwordHash: undefined },
      });

      return { ...updatedUser, passwordHash: undefined };
    } catch (error) {
      logger.error('ç”¨æˆ·ä¿¡æ¯æ›´æ–°å¤±è´¥', error, { userId });
      throw error;
    }
  }

  async deleteUser(userId) {
    try {
      const user = this.users.get(userId);
      if (!user) {
        throw new Error(`ç”¨æˆ·ä¸å­˜åœ¨: ${userId}`);
      }

      // ç»“æŸæ‰€æœ‰ç›¸å…³ä¼šè¯
      for (const [sessionId, session] of this.sessions) {
        if (session.userId === userId) {
          await this.endSession(sessionId);
        }
      }

      // åˆ é™¤ç”¨æˆ·
      this.users.delete(userId);
      this.state.setState((state) => {
        const newUsers = new Map(state.users);
        newUsers.delete(userId);
        return { users: newUsers };
      });

      logger.info('ç”¨æˆ·åˆ é™¤æˆåŠŸ', {
        userId,
        username: user.username,
      });

      // å‘å¸ƒåˆ é™¤äº‹ä»¶
      await this.messaging.publish('user.deleted', {
        userId,
        username: user.username,
      });
    } catch (error) {
      logger.error('ç”¨æˆ·åˆ é™¤å¤±è´¥', error, { userId });
      throw error;
    }
  }

  async getUserProfile(userId) {
    const user = this.users.get(userId);
    if (!user) {
      throw new Error(`ç”¨æˆ·ä¸å­˜åœ¨: ${userId}`);
    }

    return { ...user, passwordHash: undefined };
  }

  async listUsers(filters = {}) {
    let users = Array.from(this.users.values());

    // åº”ç”¨è¿‡æ»¤å™¨
    if (filters.status) {
      users = users.filter((user) => user.status === filters.status);
    }

    if (filters.role) {
      users = users.filter((user) => user.role === filters.role);
    }

    // ç§»é™¤å¯†ç å“ˆå¸Œ
    return users.map((user) => ({ ...user, passwordHash: undefined }));
  }

  // ä¼šè¯ç®¡ç†
  async endSession(sessionId) {
    const session = this.sessions.get(sessionId);
    if (session) {
      // å¯ä»¥åœ¨è¿™é‡Œæ·»åŠ ä¼šè¯æ¸…ç†é€»è¾‘
      this.sessions.delete(sessionId);
    }
  }

  async cleanupExpiredSessions() {
    const now = new Date();
    const maxSessionAge = 24 * 60 * 60 * 1000; // 24å°æ—¶

    for (const [sessionId, session] of this.sessions) {
      const age = now - session.createdAt;
      if (age > maxSessionAge) {
        await this.endSession(sessionId);
        logger.info('æ¸…ç†è¿‡æœŸä¼šè¯', { sessionId, userId: session.userId });
      }
    }
  }

  // è¾…åŠ©æ–¹æ³•
  validateUserData(userData) {
    const required = ['username', 'email', 'password'];
    for (const field of required) {
      if (!userData[field]) {
        throw new Error(`ç¼ºå°‘å¿…è¦å­—æ®µ: ${field}`);
      }
    }

    // éªŒè¯é‚®ç®±æ ¼å¼
    const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
    if (!emailRegex.test(userData.email)) {
      throw new Error('é‚®ç®±æ ¼å¼ä¸æ­£ç¡®');
    }

    // éªŒè¯å¯†ç å¼ºåº¦
    if (userData.password.length < 8) {
      throw new Error('å¯†ç é•¿åº¦è‡³å°‘8ä½');
    }
  }

  findUserByUsername(username) {
    for (const user of this.users.values()) {
      if (user.username === username) {
        return user;
      }
    }
    return null;
  }

  generateUserId() {
    return `user_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`;
  }

  generateSessionId() {
    return `session_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`;
  }

  async hashPassword(password) {
    // åœ¨ç”Ÿäº§ç¯å¢ƒä¸­ï¼Œåº”è¯¥ä½¿ç”¨bcryptæˆ–å…¶ä»–å®‰å…¨çš„å“ˆå¸Œç®—æ³•
    // è¿™é‡Œä¸ºäº†ç®€åŒ–ï¼Œä½¿ç”¨ä¸€ä¸ªåŸºæœ¬çš„å®ç°
    const crypto = await import('crypto');
    const salt = crypto.randomBytes(16).toString('hex');
    const hash = crypto
      .createHash('sha256')
      .update(password + salt)
      .digest('hex');
    return `${salt}:${hash}`;
  }

  async verifyPassword(password, passwordHash) {
    const crypto = await import('crypto');
    const [salt, hash] = passwordHash.split(':');
    const expectedHash = crypto
      .createHash('sha256')
      .update(password + salt)
      .digest('hex');
    return hash === expectedHash;
  }

  updateUserState(userId, user) {
    this.state.setState((state) => ({
      users: new Map(state.users).set(userId, user),
    }));
  }

  // å®šæœŸæ¸…ç†ä»»åŠ¡
  startCleanupTask() {
    setInterval(
      async () => {
        try {
          await this.cleanupExpiredSessions();
        } catch (error) {
          logger.error('ä¼šè¯æ¸…ç†ä»»åŠ¡å¤±è´¥', error);
        }
      },
      60 * 60 * 1000,
    ); // æ¯å°æ—¶æ¸…ç†ä¸€æ¬¡
  }
}
