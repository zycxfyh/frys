#!/usr/bin/env node

/**
 * WokeFlow Production - ä¸»å…¥å£æ–‡ä»¶
 * ä¼ä¸šçº§å·¥ä½œæµç®¡ç†ç³»ç»Ÿ (è½»é‡åŒ–æ¶æ„)
 */

import {
  LightweightContainer,
  Service,
  Dependency,
} from './core/LightweightContainer.js';
import { errorHandler } from './core/UnifiedErrorHandler.js';
import { pluginManager } from './core/PluginManager.js';
import { EventStream } from './core/FunctionalUtils.js';
import { logger } from './utils/logger.js';
import { config } from './utils/config.js';

// å¯¼å…¥æ ¸å¿ƒæ¨¡å—
import AxiosInspiredHTTP from './core/AxiosInspiredHTTP.js';
import NATSInspiredMessaging from './core/NATSInspiredMessaging.js';
import ZustandInspiredState from './core/ZustandInspiredState.js';
import JWTInspiredAuth from './core/JWTInspiredAuth.js';
import DayJSInspiredDate from './core/DayJSInspiredDate.js';
import LodashInspiredUtils from './core/LodashInspiredUtils.js';

// å¯¼å…¥æœåŠ¡
import { WorkflowEngine } from './services/WorkflowEngine.js';
import { UserService } from './services/UserService.js';

// åˆ›å»ºå…¨å±€ä¾èµ–æ³¨å…¥å®¹å™¨
const container = new LightweightContainer();

// åˆ›å»ºå…¨å±€äº‹ä»¶æµ
const eventStream = new EventStream();

// æ³¨å†Œæ ¸å¿ƒæ¨¡å—åˆ°å®¹å™¨
container.register('http', AxiosInspiredHTTP);
container.register('messaging', NATSInspiredMessaging);
container.register('state', ZustandInspiredState);
container.register('auth', JWTInspiredAuth);
container.register('date', DayJSInspiredDate);
container.register('utils', LodashInspiredUtils);
container.register('eventStream', eventStream);
container.register('config', config);
container.register('logger', logger);

// æ³¨å†ŒæœåŠ¡ (ä½¿ç”¨ä¾èµ–æ³¨å…¥)
// æ³¨æ„: è£…é¥°å™¨è¯­æ³•åœ¨å½“å‰ESLinté…ç½®ä¸­ä¸æ”¯æŒï¼Œå·²æ³¨é‡Š
// @Dependency('http', 'messaging', 'state', 'date', 'utils')
// @Service('workflowEngine')
class WorkflowEngineService extends WorkflowEngine {}

container.register('workflowEngine', new WorkflowEngineService());

// @Dependency('http', 'auth', 'state', 'messaging')
// @Service('userService')
class UserServiceInjected extends UserService {}

container.register('userService', new UserServiceInjected());

/**
 * WokeFlow Production - è½»é‡åŒ–åº”ç”¨ç±»
 */
class WokeFlowProduction {
  constructor() {
    this.container = container;
    this.eventStream = eventStream;
    this.initialized = false;
    this.services = new Map();
  }

  async initialize() {
    try {
      logger.info('ğŸš€ åˆå§‹åŒ– WokeFlow Production ç³»ç»Ÿ (è½»é‡åŒ–æ¶æ„)');

      // 1. åˆå§‹åŒ–æ ¸å¿ƒæ¨¡å— (é€šè¿‡ä¾èµ–æ³¨å…¥å®¹å™¨)
      const coreModules = [
        'http',
        'messaging',
        'state',
        'auth',
        'date',
        'utils',
      ];
      for (const moduleName of coreModules) {
        const module = this.container.resolve(moduleName);
        await module.initialize();
        this.services.set(moduleName, module);
        logger.debug(`æ ¸å¿ƒæ¨¡å—å·²åˆå§‹åŒ–: ${moduleName}`);
      }

      // è®¾ç½®è®¤è¯å¯†é’¥
      const auth = this.services.get('auth');
      auth.setSecret('default', config.auth.secret);

      // 2. åˆå§‹åŒ–ä¸šåŠ¡æœåŠ¡ (é€šè¿‡ä¾èµ–æ³¨å…¥)
      const services = ['workflowEngine', 'userService'];
      for (const serviceName of services) {
        const service = this.container.resolve(serviceName);
        await service.initialize();
        this.services.set(serviceName, service);
        logger.debug(`ä¸šåŠ¡æœåŠ¡å·²åˆå§‹åŒ–: ${serviceName}`);
      }

      // 3. è®¾ç½®äº‹ä»¶ç›‘å¬
      await this.setupEventListeners();

      // 4. åˆå§‹åŒ–æ’ä»¶ç³»ç»Ÿ
      await pluginManager.initialize();
      await pluginManager.setContext({
        container: this.container,
        eventStream: this.eventStream,
        config,
        logger,
      });

      // 5. æ›´æ–°ç³»ç»ŸçŠ¶æ€
      const state = this.services.get('state');
      state.setState((currentState) => ({
        system: {
          ...currentState.system,
          status: 'ready',
          version: '2.0.0-lightweight',
          architecture: 'container-based',
        },
      }));

      this.initialized = true;
      logger.info('âœ… WokeFlow Production ç³»ç»Ÿåˆå§‹åŒ–å®Œæˆ (è½»é‡åŒ–æ¶æ„)');
    } catch (error) {
      await errorHandler.handle(error, { context: 'system_initialization' });
      throw error;
    }
  }

  async setupEventListeners() {
    const messaging = this.services.get('messaging');

    // ä½¿ç”¨äº‹ä»¶æµç»Ÿä¸€ç®¡ç†äº‹ä»¶
    this.eventStream.on('user.created', (user) => {
      logger.info('æ–°ç”¨æˆ·åˆ›å»º', { userId: user.id, username: user.username });
      messaging.publish('user.created', user);
    });

    this.eventStream.on('workflow.started', (workflow) => {
      logger.info('å·¥ä½œæµå¯åŠ¨', {
        workflowId: workflow.id,
        name: workflow.name,
      });
      messaging.publish('workflow.started', workflow);
    });

    this.eventStream.on('task.completed', (task) => {
      logger.info('ä»»åŠ¡å®Œæˆ', { taskId: task.id, workflowId: task.workflowId });
      messaging.publish('task.completed', task);
    });

    this.eventStream.on('system.error', async (error) => {
      logger.error('ç³»ç»Ÿé”™è¯¯', error);
      await errorHandler.handle(error, { context: 'system_event' });
    });

    // ç›‘å¬æ’ä»¶äº‹ä»¶
    pluginManager.hook('plugin:registered', (data) => {
      logger.info(`æ’ä»¶å·²æ³¨å†Œ: ${data.plugin.name}`);
    });

    pluginManager.hook('plugin:started', (data) => {
      logger.info(`æ’ä»¶å·²å¯åŠ¨: ${data.plugin.name}`);
    });
  }

  async start() {
    if (!this.initialized) {
      await this.initialize();
    }

    try {
      logger.info('ğŸ¯ WokeFlow Production ç³»ç»Ÿå¯åŠ¨ (è½»é‡åŒ–æ¶æ„)');

      // å¯åŠ¨æ ¸å¿ƒæ¨¡å—
      const coreModules = [
        'http',
        'messaging',
        'state',
        'auth',
        'date',
        'utils',
      ];
      for (const moduleName of coreModules) {
        const module = this.services.get(moduleName);
        await module.start();
        logger.debug(`æ ¸å¿ƒæ¨¡å—å·²å¯åŠ¨: ${moduleName}`);
      }

      // å¯åŠ¨ä¸šåŠ¡æœåŠ¡
      const services = ['workflowEngine', 'userService'];
      await Promise.all(
        services.map(async (serviceName) => {
          const service = this.services.get(serviceName);
          await service.start();
          logger.debug(`ä¸šåŠ¡æœåŠ¡å·²å¯åŠ¨: ${serviceName}`);
        }),
      );

      // å¯åŠ¨æ’ä»¶ç³»ç»Ÿ
      await pluginManager.startAll();

      logger.info('âœ… æ‰€æœ‰æœåŠ¡å¯åŠ¨å®Œæˆ (è½»é‡åŒ–æ¶æ„)');

      // ä¿æŒè¿›ç¨‹è¿è¡Œ
      this.keepAlive();
    } catch (error) {
      await errorHandler.handle(error, { context: 'system_start' });
      throw error;
    }
  }

  keepAlive() {
    // å®šæœŸå¥åº·æ£€æŸ¥
    this.healthCheckTimer = setInterval(async () => {
      try {
        const health = await this.healthCheck();
        if (!health.healthy) {
          logger.warn('å¥åº·æ£€æŸ¥å¤±è´¥', health);
          this.eventStream.emit('system:health_check_failed', health);
        } else {
          this.eventStream.emit('system:health_check_passed', health);
        }
      } catch (error) {
        logger.error('å¥åº·æ£€æŸ¥å¼‚å¸¸', error);
        await errorHandler.handle(error, { context: 'health_check' });
      }
    }, 30000); // æ¯30ç§’æ£€æŸ¥ä¸€æ¬¡

    // ä¼˜é›…å…³é—­å¤„ç†
    process.on('SIGTERM', () => this.gracefulShutdown('SIGTERM'));
    process.on('SIGINT', () => this.gracefulShutdown('SIGINT'));
    process.on('uncaughtException', (error) => {
      logger.error('æœªæ•è·çš„å¼‚å¸¸', error);
      this.gracefulShutdown('uncaughtException');
    });
    process.on('unhandledRejection', (reason, promise) => {
      logger.error('æœªå¤„ç†çš„Promiseæ‹’ç»', { reason, promise });
      this.gracefulShutdown('unhandledRejection');
    });
  }

  async healthCheck() {
    const checks = {
      timestamp: Date.now(),
      services: {},
      container: this.container.getStatus(),
      plugins: pluginManager.getAllStatuses(),
    };

    try {
      // æ£€æŸ¥å„ä¸ªæœåŠ¡çŠ¶æ€
      const serviceNames = [
        'http',
        'messaging',
        'state',
        'auth',
        'date',
        'utils',
        'workflowEngine',
        'userService',
      ];
      for (const serviceName of serviceNames) {
        const service = this.services.get(serviceName);
        if (service && typeof service.healthCheck === 'function') {
          checks.services[serviceName] = await service.healthCheck();
        } else {
          checks.services[serviceName] = service ? 'healthy' : 'unhealthy';
        }
      }

      // æ£€æŸ¥ç³»ç»ŸçŠ¶æ€
      const state = this.services.get('state');
      const systemState = state ? state.getState() : {};
      checks.services.system =
        systemState.system?.status === 'ready' ? 'healthy' : 'unhealthy';

      // æ€»ä½“å¥åº·çŠ¶æ€
      const serviceStatuses = Object.values(checks.services);
      checks.healthy = serviceStatuses.every(
        (status) =>
          status === 'healthy' ||
          (typeof status === 'object' && status.healthy !== false),
      );

      checks.architecture = 'lightweight';
      checks.version = '2.0.0';
    } catch (error) {
      checks.healthy = false;
      checks.error = error.message;
      logger.error('å¥åº·æ£€æŸ¥å¤±è´¥', error);
    }

    return checks;
  }

  async gracefulShutdown(signal) {
    logger.info(`æ”¶åˆ° ${signal} ä¿¡å·ï¼Œå¼€å§‹ä¼˜é›…å…³é—­ (è½»é‡åŒ–æ¶æ„)`);

    try {
      // æ¸…ç†å®šæ—¶å™¨
      if (this.healthCheckTimer) {
        clearInterval(this.healthCheckTimer);
      }

      // åœæ­¢æ’ä»¶ç³»ç»Ÿ
      await pluginManager.stopAll();

      // åœæ­¢ä¸šåŠ¡æœåŠ¡
      const services = ['userService', 'workflowEngine'];
      await Promise.all(
        services.map(async (serviceName) => {
          const service = this.services.get(serviceName);
          if (service && typeof service.stop === 'function') {
            await service.stop();
            logger.debug(`ä¸šåŠ¡æœåŠ¡å·²åœæ­¢: ${serviceName}`);
          }
        }),
      );

      // åœæ­¢æ ¸å¿ƒæ¨¡å—
      const coreModules = [
        'utils',
        'date',
        'auth',
        'state',
        'messaging',
        'http',
      ];
      for (const moduleName of coreModules.reverse()) {
        const module = this.services.get(moduleName);
        if (module && typeof module.stop === 'function') {
          await module.stop();
          logger.debug(`æ ¸å¿ƒæ¨¡å—å·²åœæ­¢: ${moduleName}`);
        }
      }

      // æ¸…ç†å®¹å™¨
      this.container.clear();

      logger.info('âœ… ç³»ç»Ÿä¼˜é›…å…³é—­å®Œæˆ (è½»é‡åŒ–æ¶æ„)');
      process.exit(0);
    } catch (error) {
      logger.error('ä¼˜é›…å…³é—­å¤±è´¥', error);
      await errorHandler.handle(error, {
        context: 'graceful_shutdown',
        signal,
      });
      process.exit(1);
    }
  }

  // å…¬å¼€APIæ¥å£ (è½»é‡åŒ–æ¶æ„)

  /**
   * è·å–æ‰€æœ‰æœåŠ¡
   */
  getServices() {
    const services = {};
    for (const [name, service] of this.services) {
      services[name] = service;
    }
    return services;
  }

  /**
   * è·å–æŒ‡å®šæœåŠ¡
   */
  getService(name) {
    return this.services.get(name);
  }

  /**
   * è·å–å®¹å™¨å®ä¾‹
   */
  getContainer() {
    return this.container;
  }

  /**
   * è·å–äº‹ä»¶æµ
   */
  getEventStream() {
    return this.eventStream;
  }

  /**
   * è·å–æ’ä»¶ç®¡ç†å™¨
   */
  getPluginManager() {
    return pluginManager;
  }

  /**
   * è·å–ç³»ç»ŸçŠ¶æ€
   */
  getSystemStatus() {
    return {
      initialized: this.initialized,
      services: this.services.size,
      container: this.container.getStatus(),
      plugins: pluginManager.getAllStatuses(),
      architecture: 'lightweight',
      version: '2.0.0',
    };
  }
}

// åˆ›å»ºå…¨å±€å®ä¾‹
const app = new WokeFlowProduction();

// å¯¼å‡ºä¾›å¤–éƒ¨ä½¿ç”¨
export { app, WokeFlowProduction };

// å¦‚æœç›´æ¥è¿è¡Œæ­¤æ–‡ä»¶
if (import.meta.url === `file://${process.argv[1]}`) {
  app.start().catch((error) => {
    logger.error('åº”ç”¨å¯åŠ¨å¤±è´¥', error);
    process.exit(1);
  });
}
