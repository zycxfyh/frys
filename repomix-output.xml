  async benchmarkModule(moduleName, testFn) {
    try {
      console.log(`ğŸ“Š æµ‹è¯• ${moduleName}...`);
      const duration = await testFn();
      this.results[moduleName] = {
        duration: Math.round(duration * 100) / 100,
        status: 'success'
