# frys 测试环境配置
# 用于工业级测试的模拟配置

NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# JWT 配置
JWT_SECRET=test-jwt-secret-key-for-testing-only

# AI 服务配置 - 使用测试/模拟密钥
OPENAI_API_KEY=test-openai-key
CLAUDE_API_KEY=test-claude-key
GEMINI_API_KEY=test-gemini-key
DEEPSEEK_API_KEY=test-deepseek-key
ALIBABA_API_KEY=test-alibaba-key
COGNEE_API_KEY=test-cognee-key
COGNEE_PROJECT_ID=test-project-123

# 数据库配置 - 使用内存数据库用于测试
DB_TYPE=memory

# 缓存配置 - 禁用缓存用于测试
CACHE_TYPE=memory

# 监控配置 - 禁用监控用于测试
MONITORING_ENABLED=false

# 安全配置 - 放宽限制用于测试
RATE_LIMIT_MAX=1000
CORS_ORIGIN=*
