            shift
            ;;
        --help)
            echo "WokeFlow è“ç»¿éƒ¨ç½²è„šæœ¬"
            echo ""
            echo "ç”¨æ³•: $0 [é€‰é¡¹]"
            echo ""
            echo "é€‰é¡¹:"
            echo "  --env=ENV        éƒ¨ç½²ç¯å¢ƒ (é»˜è®¤: production)"
            echo "  --help           æ˜¾ç¤ºæ­¤å¸®åŠ©ä¿¡æ¯"
            exit 0
            ;;
        *)
            error "æœªçŸ¥é€‰é¡¹: $1"
            exit 1
            ;;
