#!/usr/bin/env node

/**
 * WokeFlow PR æ£€æŸ¥è„šæœ¬
 * è‡ªåŠ¨åŒ–æ‰§è¡ŒPRè´¨é‡æ£€æŸ¥
 */

import { execSync } from 'child_process';
import fs from 'fs';
import path from 'path';

const __dirname = path.dirname(new URL(import.meta.url).pathname);

class PRChecker {
  constructor() {
    this.errors = [];
    this.warnings = [];
    this.results = {
      passed: 0,
      failed: 0,
      warnings: 0
    };
  }

  log(message, type = 'info') {
    const timestamp = new Date().toISOString();
    const prefix = {
      info: 'â„¹ï¸ ',
      success: 'âœ… ',
      warning: 'âš ï¸ ',
      error: 'âŒ '
    }[type] || 'â„¹ï¸ ';

    console.log(`[${timestamp}] ${prefix}${message}`);
  }

  runCommand(command, description) {
    try {
      this.log(`æ‰§è¡Œ: ${description}`, 'info');
      const result = execSync(command, { encoding: 'utf8', stdio: 'pipe' });
      this.log(`${description} - æˆåŠŸ`, 'success');
      this.results.passed++;
      return result;
    } catch (error) {
      this.log(`${description} - å¤±è´¥: ${error.message}`, 'error');
      this.errors.push(`${description}: ${error.message}`);
      this.results.failed++;
      return null;
    }
  }

  checkESLint() {
    return this.runCommand('npm run lint', 'ESLintä»£ç è§„èŒƒæ£€æŸ¥');
  }

  checkFormatting() {
    return this.runCommand('npm run format:check', 'Prettierä»£ç æ ¼å¼åŒ–æ£€æŸ¥');
  }

  checkUnitTests() {
    return this.runCommand('npm run test:unit', 'å•å…ƒæµ‹è¯•æ‰§è¡Œ');
  }

  checkIntegrationTests() {
    return this.runCommand('npm run test:integration', 'é›†æˆæµ‹è¯•æ‰§è¡Œ');
  }

  checkSecurityTests() {
    return this.runCommand('npm run test:security', 'å®‰å…¨æµ‹è¯•æ‰§è¡Œ');
  }

  checkSecurityAudit() {
    return this.runCommand('npm audit --audit-level high', 'å®‰å…¨æ¼æ´æ‰«æ');
  }

  checkCoverage() {
    // å…ˆè¿è¡Œè¦†ç›–ç‡æµ‹è¯•
    const testResult = this.runCommand('npm run test:coverage', 'æµ‹è¯•è¦†ç›–ç‡æ£€æŸ¥');

    // å•ç‹¬æ£€æŸ¥è¦†ç›–ç‡é˜ˆå€¼
    try {
      const coveragePath = path.join(process.cwd(), 'coverage', 'coverage-summary.json');
      if (fs.existsSync(coveragePath)) {
        const coverage = JSON.parse(fs.readFileSync(coveragePath, 'utf8'));
        const thresholds = { branches: 80, functions: 80, lines: 80, statements: 80 };

        for (const [type, threshold] of Object.entries(thresholds)) {
          const actual = coverage.total[type].pct;
          if (actual < threshold) {
            this.warnings.push(`è¦†ç›–ç‡ä¸è¶³: ${type} ${actual}% < ${threshold}%`);
            this.results.warnings++;
          } else {
            this.log(`è¦†ç›–ç‡æ£€æŸ¥: ${type} ${actual}% >= ${threshold}%`, 'success');
          }
        }
      } else {
        this.warnings.push('è¦†ç›–ç‡æŠ¥å‘Šæ–‡ä»¶ä¸å­˜åœ¨');
        this.results.warnings++;
      }
    } catch (error) {
      this.warnings.push(`æ— æ³•è¯»å–è¦†ç›–ç‡æ•°æ®: ${error.message}`);
      this.results.warnings++;
    }

    return testResult;
  }

  checkFileSize() {
    const maxSize = 500 * 1024; // 500KB
    const files = execSync('find src -name "*.js" -type f', { encoding: 'utf8' })
      .trim()
      .split('\n')
      .filter(Boolean);

    for (const file of files) {
      try {
        const stats = fs.statSync(file);
        if (stats.size > maxSize) {
          this.warnings.push(`æ–‡ä»¶è¿‡å¤§: ${file} (${Math.round(stats.size / 1024)}KB)`);
          this.results.warnings++;
        }
      } catch (error) {
        // å¿½ç•¥æ–‡ä»¶è¯»å–é”™è¯¯
      }
    }
  }

  checkCircularDependencies() {
    try {
      // ä½¿ç”¨ç®€å•çš„å¾ªç¯ä¾èµ–æ£€æŸ¥
      const result = execSync('npx madge --circular --format cjs src/', {
        encoding: 'utf8',
        stdio: 'pipe'
      });
      if (result.trim()) {
        this.errors.push(`å‘ç°å¾ªç¯ä¾èµ–:\n${result}`);
        this.results.failed++;
      } else {
        this.log('å¾ªç¯ä¾èµ–æ£€æŸ¥ - é€šè¿‡', 'success');
        this.results.passed++;
      }
    } catch (error) {
      // madge å¯èƒ½æœªå®‰è£…ï¼Œè·³è¿‡æ­¤æ£€æŸ¥
      this.log('å¾ªç¯ä¾èµ–æ£€æŸ¥ - è·³è¿‡ (éœ€è¦å®‰è£… madge)', 'warning');
    }
  }

  generateReport() {
    const report = {
      timestamp: new Date().toISOString(),
      results: this.results,
      errors: this.errors,
      warnings: this.warnings,
      summary: {
        status: this.results.failed === 0 ? 'PASSED' : 'FAILED',
        totalChecks: this.results.passed + this.results.failed,
        passed: this.results.passed,
        failed: this.results.failed,
        warnings: this.results.warnings
      }
    };

    // ä¿å­˜æŠ¥å‘Š
    const reportPath = path.join(process.cwd(), 'pr-check-report.json');
    fs.writeFileSync(reportPath, JSON.stringify(report, null, 2));

    return report;
