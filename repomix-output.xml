          runningServices++;
        } else {
          this.addCheck(`DockeræœåŠ¡: ${service}`, 'warning', 'æœåŠ¡æœªè¿è¡Œ');
        }
      });

      return runningServices >= 3; // è‡³å°‘æ ¸å¿ƒæœåŠ¡è¿è¡Œ

    } catch (error) {
      this.addCheck('DockeræœåŠ¡æ£€æŸ¥', 'failed', error.message);
